	</modules>
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.2.4.RELEASE</version>
		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	<groupId>com.sagar</groupId>
	<artifactId>meeting-app</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>meeting-app</name>
	<description>Demo project for Meeting Room Booking</description>

	<properties>
		<java.version>1.8</java.version>
		<node.version>v12.4.0</node.version>
		<npm.version>6.9.0</npm.version>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
		<start-class>com.sagar.meetingroom.MeetingRoomApplication</start-class>
